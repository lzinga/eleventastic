{
  "name": "eleventastic",
  "version": "1.0.0",
  "description": "A boilerplate for eleventy websites",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/maxboeck/eleventastic"
  },
  "author": "Max BÃ¶ck",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-loader": "^9.1.3",
    "clean-css": "^5.3.3",
    "critical": "^7.1.0",
    "cssesc": "^3.0.0",
    "del-cli": "^5.1.0",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "focus-trap": "^7.5.4",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "memfs": "^4.6.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "svg-sprite": "^2.0.2",
    "webpack": "^5.90.0"
  },
  "devDependencies": {
    "bootstrap": "^5.3.2",
    "cross-env": "^7.0.3"
  }
}
